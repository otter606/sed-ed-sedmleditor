<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.sedml.jlibsedml.editor.product.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="SED-ED Perspective"
            class="org.sedml.jlibsedml.editor.product.Perspective"
            id="org.sedml.jlibsedml.editor.product.perspective">
      </perspective>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.sedml.jlibsedml.editor.product.application"
            name="SED-ED">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="appName"
               value="SED-ED">
         </property>
         <property
               name="aboutText"
               value="This is the SED-ED SED-ML editor v 0.9.9, February 2012">
         </property>
         <property
               name="startupProgressRect"
               value="105,275,200,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupMessageRect"
               value="85,252,200,20">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
          <!-- Help menu -->
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="help"
               label="Help"
               tooltip="Actions for help, update and about dialogs">
            <command
                  commandId="org.eclipse.ui.help.helpContents"
                  id="help.helpContents"
                  label="Help Contents"
                  style="push">
            </command>
            <separator
                  name="uk.ac.ed.csbe.help.separator1"
                  visible="true">
            </separator>
        
            <separator
                  name="uk.ac.ed.csbe.sbsivisual2.separator1">
            </separator>
                <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  id="help.helpAbout"
                  label="About SED-ED"
                  style="push">
            </command>
         
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="help"
               label="Help"
               tooltip="Show Help and information">
            <menu
                  commandId="org.eclipse.ui.help.aboutAction"
                  id="help.helpAbout"
                  label="About SED-ED">
            </menu>
         </menu>
      </menuContribution>
        <menuContribution
            locationURI="menu:file">
         <command
               commandId="org.sedml.jlibsedml.editor.product.switchworkspace"
               label="Switch workspace"
               style="push"
               tooltip="Change workspace to another folder">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.sedml.jlibsedml.editor.product.sedcategory"
            class="org.sedml.jlibsedml.editor.product.ProjectView"
            id="org.sedml.seded.projectview2"
            name="SED-ED project view"
            restorable="true">
      </view>
      <category
            id="org.sedml.jlibsedml.editor.product.sedcategory"
            name="SEDML">
      </category>
   </extension>
   
    <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="org.sedml.seded.projectview2">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="org.sedml.seded.projectview2">
         <includes>
            <contentExtension
                  isRoot="false"
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
             <contentExtension
                  pattern="org.sedml.jlibsedml.plugin.sedmlContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerContentBinding
            viewerId="org.sedml.seded.projectview2">
         <includes>
            <contentExtension
                  pattern="org.sedml.jlibsedml.plugin.sedxContent">
            </contentExtension>
         </includes>
         
      
      </viewerContentBinding>

      
      
   </extension>
    <extension
           point="org.eclipse.ui.commands">
        <command
              defaultHandler="org.sedml.jlibsedml.editor.product.SwitchWorkspaceHandler"
              id="org.sedml.jlibsedml.editor.product.switchworkspace"
              name="switchworkspace command">
        </command>
     </extension>
    <extension
          point="org.eclipse.help.toc">
       <toc
             file="help/SED-ML_EditorApp-toc.xml"
             primary="true">
       </toc>
    </extension>
   
   
    

</plugin>
